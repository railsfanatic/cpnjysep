            <li><%= nav_link "Activities", activities_path %></li>
            <li><%= nav_link "What is JYSEP?", about_path, true %></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div><!--/.container-->
    </div><!--/.navbar-inner-->
  </div><!--/.navbar-->

  <div class="container">
